import java.io.*;
import java.lang.*;

public class MagnitudeFilter implements Filter {
    private double min;
    private double max;
    private String name;
    
    public MagnitudeFilter(double minMag, double maxMag, String name) { 
        min = minMag;
        max = maxMag;
        name =name;
    } 

    public boolean satisfies (QuakeEntry qe) {
        
        return qe.getMagnitude() >= min &&
               qe.getMagnitude() <= max;
    } 
    
    public String getName() {
        return name;
    }
}
